                            System.out.println("Which one do you want to take ?");
                            i=input.nextInt();
                            OK=true;
                        }
                        catch(InputMismatchException e){
                        }
                        if(J1.getSkills().contains((Skill)J2.getSkills().get(i))){
                            System.out.println("You already got it");
                            OK=false;
                        }
                    }while(i<0 || J2.SkillList().length()<i || !(OK) );
