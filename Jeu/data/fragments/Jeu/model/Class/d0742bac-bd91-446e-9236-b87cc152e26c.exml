                }
            }
            if(!(J2.getSkills().size()<=J2.getNbCap())){
                do{
                    System.out.println(J2.getName()+" : You have to remove one of your's skill");
                    System.out.println(J2.SkillList());
                    i=input.nextInt();
                }while(i<0 || J2.SkillList().length()<i);
                J2.getSkills().remove(i);
            }
