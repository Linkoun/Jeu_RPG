                  <ATT name="Content"><![CDATA[/*    
    public void StartFight(){
        C1.setCharacter();
        C2.setCharacter();
        boolean CombatFini=false;
        Character J1=null;
        Character J2=null;
        if(C1.getEXP()>C2.getEXP()){
